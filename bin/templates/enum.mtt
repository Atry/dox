::use 'main.mtt'::
	<h1>
		<code>
			<span class="directive">enum</span> ::enum.path::
		</code>
	</h1>
	<div class="doc" ::cond class.doc != null::>
		::raw class.doc::
	</div>
	::if enum.constructors.length > 0::
		<h2>Constructors</h2>
		::foreach ctor enum.constructors::
			::use "enum_field.mtt"::::end::
		::end::
	::end::
::end::